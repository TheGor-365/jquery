<script type="text/javascript">

  $(document).ready(() => {

    $('.photo-shoe-details').show();

    $('.photo-more-details-button').on('click', event => {

    });

    $('.photo-shoe-details li').on('click', event => {
      $(event.currentTarget).addClass('photo-active');

      $(event.currentTarget).siblings().removeClass('photo-active')

      $('.photo-shoe-details').children().removeClass('photo-disabled')
    });


    $('.photo-login-button').on('click', () => {
      $('.photo-login-form').toggle();
      $('.photo-login-button').toggleClass('photo-button-active');
    });

    $('.photo-product-photo').on('mouseenter', () => {
      $(this).addClass('photo-photo-active');
    }).on('mouseleave', function() {
      $(this).removeClass('photo-photo-active');
    });

    $('.photo-menu-button').on('click', () => {
      $('.photo-menu-button').toggleClass('photo-button-active');
      $('.photo-nav-menu').toggleClass('photo-hide');
    });

  })

</script>
